@model Gensci_LAM.ViewModel.BookVMs.BookVM
@inject IStringLocalizer<Program> Localizer;

<wt:form vm="@Model">
    <wt:row items-per-row="ItemsPerRowEnum.Two">
        <wt:display field="Entity.BookName" />
        <wt:display field="Entity.BookType" />
        <wt:display field="Entity.Remark" />
        <wt:multiupload field="Entity.Photos" disabled="true" upload-type="ImageFile" />

    </wt:row>
    <wt:row align="AlignEnum.Right">
        <wt:closebutton />
    </wt:row>
</wt:form>
