@model Gensci_LAM.ViewModel.StudentMdlVMs.StudentMdlVM
@inject IStringLocalizer<Program> Localizer;

<wt:form vm="@Model">
    <wt:row items-per-row="ItemsPerRowEnum.Two">
        <wt:textbox field="Entity.LoginName" />
        <wt:textbox field="Entity.Password" />
        <wt:textbox field="Entity.Name" />
        <wt:textbox field="Entity.CellPhone" />
        <wt:textbox field="Entity.ZipCode" />
        <wt:datetime field="Entity.EnRollDate" />
        <wt:upload field="Entity.PhotoId" />
        <wt:checkbox field="SelectedStudentMajorIDs" items="AllStudentMajors" />
        <wt:combobox field="SelectedStudentMajorIDs" multi-select="false" items="AllStudentMajors" />
    </wt:row>
    <wt:row align="AlignEnum.Right">
        <wt:submitbutton />
        <wt:closebutton />
    </wt:row>
</wt:form>
